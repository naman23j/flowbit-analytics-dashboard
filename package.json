{
  "name": "flowbit-analytics-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "Full-stack analytics dashboard with AI-powered chat interface",
  "workspaces": [
    "apps/*",
    "services/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "start": "npm run start --workspaces --if-present",
    "db:migrate": "npm run db:migrate -w apps/api",
    "db:seed": "npm run db:seed -w apps/api",
    "dev:api": "npm run dev -w apps/api",
    "dev:web": "npm run dev -w apps/web",
    "dev:vanna": "npm run dev -w services/vanna"
  },
  "keywords": ["analytics", "dashboard", "ai", "groq", "nextjs", "express"],
  "author": "Flowbit Internship",
  "license": "MIT"
}
